{
    "name": "oligus/doctrine-fixtures",
    "description": "Doctrine fixtures",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "oligus",
            "email": "oli.gustafsson@gmail.com"
        }
    ],
    "config": {
        "platform": {
            "php": "7.2"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "coverage": "vendor/bin/phpunit --coverage-clover=coverage.xml",
        "watch": "vendor/bin/phpunit-watcher watch",
        "lint": "vendor/bin/phpcs --standard=ruleset.xml ./src",
        "lint-fix": "vendor/bin/phpcbf --standard=ruleset.xml ./src",
        "analyze": "vendor/bin/phan --color --progress-bar"
    },
    "require": {
        "php": ">=7.2",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "ext-json": "*",
        "phpunit/phpunit": "^8.3",
        "doctrine/orm": "^2.6",
        "symfony/var-dumper": "^4.3",
        "spatie/phpunit-snapshot-assertions": "^2.1",
        "squizlabs/php_codesniffer": "^3.4",
        "phan/phan": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "DoctrineFixtures\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests",
            "Tests\\Src\\": "tests/src",
            "Tests\\Doctrine\\": "tests/Doctrine"
        },
        "classmap": [
            "tests/src"
        ]
    }
}
